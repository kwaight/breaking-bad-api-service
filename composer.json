{
    "name": "kwaight/breaking-bad-api-service",
    "description": "Make it easier to connect to the Breaking Bad API within Laravel",
    "require": {
        "php": "^7.3",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kiefer Waight",
            "email": "kiefer.waight@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "kwaight\\BreakingBadApiService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kwaight\\BreakingBadApiService\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "kwaight\\BreakingBadApiService\\BreakingBadApiServiceProvider"
            ]
        }
    }
}
